    "version": "0.2.0",
    "configurations": [
        {
            "name": "dev_places_app",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "program": "lib/main.dart",
            "args": [
                "--flavor",
                "dev"
            ]
        },
        {
            "name": "profile_places_app",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "program": "lib/main_profile.dart",
            "args": [
                "--flavor",
                "preprod"
            ]
        },
        {
            "name": "prod_places_app",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "program": "lib/main_prod.dart",
            "args": [
                "--flavor",
                "prod"
            ]

        }
    ]
}